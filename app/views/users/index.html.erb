<div id="user-list">
  <h1>Users:</h1>
  <ul class="list-group">
    <% @users.each do |user| %>
      <li class="list-group-item">
        <span><%= user.name %></span>
        <span style="float: right;"><a href="<%= user_path(user.id) %>">Go to his page</a></span>
      </li>
    <% end %>
  </ul>
</div>
